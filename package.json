{
  "name": "webpack-es6",
  "version": "1.0.0",
  "description": "starter kit for webpack-es6",
  "main": "app.js",
  "scripts": {
    "start": "node --use_strict server.js",
    "go": "npm-run-all --parallel webpack-watcher server-dev",
    "server-dev": "NODE_ENV=development nodemon server.js",
    "webpack-watcher": "webpack --progress --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "gen-seed-data" : "NODE_ENV=development node ./src-server/db/seed.js",
    "set-project-name" : "node ./src-server/cli/setProjectName.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "backbone": "^1.3.3",
    "jquery": "^2.2.4",
    "mongoose": "^4.8.3",
    "webpack-dev-middleware": "^1.8.4"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.26.1",
    "ejs": "^2.4.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "prompt": "^1.0.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1"
  }
}
